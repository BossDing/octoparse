<!--基础元素-->
<template name="maxParseVideo">
  <!--增加video标签支持，并循环添加-->
  <view class="{{item.classStr}} maxParse-{{item.tag}}" style="{{item.styleStr}}">
    <video class="{{item.classStr}} maxParse-{{item.tag}}-video" src="{{item.attr.src}}"></video>
  </view>
</template>

<template name="maxParseImg">
  <image data-tag="{{item.tag}}" data-class="{{item.classStr}}" data-id="{{item.attr.id}}" class="{{item.classStr}} maxParse-{{item.tag}}" data-from="{{item.from}}" data-src="{{item.attr.src}}" data-idx="{{item.imgIndex}}" src="{{item.attr.src}}" mode="aspectFit"  bindtap="agentFunc"  style="{{item.styleStr}}"
  />
</template>

<template name="WxEmojiView">
  <view class="WxEmojiView maxParse-inline" style="{{item.styleStr}}">
    <block wx:for="{{item.textArray}}" wx:key="">
      <block class="{{item.text == '\\n' ? 'maxParse-hide':''}}" wx:if="{{item.node == 'text'}}">{{item.text}}</block>
      <block wx:elif="{{item.node == 'element'}}">
        <image class="wxEmoji" src="{{item.baseSrc}}{{item.text}}" />
      </block>
    </block>
  </view>
</template>

<template name="maxParseBr">
  <text>\n</text>
</template>
<!--入口模版-->

<template name="maxParse">
  <block wx:for="{{maxParseData}}" wx:key="">
    <template is="maxParse0" data="{{item}}" />
  </block>
</template>


<!--循环模版-->
<template name="maxParse0">
  <!--<template is="maxParse1" data="{{item}}" />-->
  <!--判断是否是标签节点-->
  <block wx:if="{{item.node == 'element'}}">
    <block wx:if="{{item.tag == 'button'}}">
      <button type="default" size="mini">
        <block wx:for="{{item.nodes}}" wx:for-item="item" wx:key="">
          <template is="maxParse1" data="{{item}}" />
        </block>
      </button>
    </block>
    <!--li类型-->
    <block wx:elif="{{item.tag == 'li'}}">
      <view data-tag="{{item.tag}}" data-class="{{item.classStr}}" data-id="{{item.attr.id}}" bindtap="agentFunc" class="{{item.classStr}} maxParse-li" style="{{item.styleStr}}">
        <view class="{{item.classStr}} maxParse-li-inner">
          <view class="{{item.classStr}} maxParse-li-text">
            <view class="{{item.classStr}} maxParse-li-circle"></view>
          </view>
          <view class="{{item.classStr}} maxParse-li-text">
            <block wx:for="{{item.nodes}}" wx:for-item="item" wx:key="">
              <template is="maxParse1" data="{{item}}" />
            </block>
          </view>
        </view>
      </view>
    </block>

    <!--video类型-->
    <block wx:elif="{{item.tag == 'video'}}">
      <template is="maxParseVideo" data="{{item}}" />
    </block>

    <!--img类型-->
    <block wx:elif="{{item.tag == 'img'}}">
      <template is="maxParseImg" data="{{item}}" />
    </block>

    <!--a类型-->
    <block data-tag="{{item.tag}}" wx:elif="{{item.tag == 'a'}}">
      <view  data-tag="{{item.tag}}" data-class="{{item.classStr}}" data-id="{{item.attr.id}}" bindtap="agentFunc" class="maxParse-inline {{item.classStr}} maxParse-{{item.tag}}" data-src="{{item.attr.href}}" style="{{item.styleStr}}">
        <block wx:for="{{item.nodes}}" wx:for-item="item" wx:key="">
          <template is="maxParse1" data="{{item}}" />
        </block>
      </view>
    </block>
    <block wx:elif="{{item.tag == 'table'}}">
      <view class="{{item.classStr}} maxParse-{{item.tag}}" style="{{item.styleStr}}">
        <block wx:for="{{item.nodes}}" wx:for-item="item" wx:key="">
          <template is="maxParse1" data="{{item}}" />
        </block>
      </view>
    </block>

    <block wx:elif="{{item.tag == 'br'}}">
      <template is="maxParseBr"></template>
    </block>
    <!--其他块级标签-->
    <block data-tag="{{item.tag}}" data-class="{{item.classStr}}" data-id="{{item.attr.id}}" bindtap="agentFunc" wx:elif="{{item.tagType == 'block'}}">
      <view data-tag="{{item.tag}}" data-class="{{item.classStr}}" data-id="{{item.attr.id}}" bindtap="agentFunc" class="{{item.classStr}} maxParse-{{item.tag}}" style="{{item.styleStr}}">
        <block wx:for="{{item.nodes}}" wx:for-item="item" wx:key="">
          <template is="maxParse1" data="{{item}}" />
        </block>
      </view>
    </block>

    <!--内联标签-->
    <view data-tag="{{item.tag}}" data-class="{{item.classStr}}" data-id="{{item.attr.id}}" bindtap="agentFunc" wx:else class="{{item.classStr}} maxParse-{{item.tag}} maxParse-{{item.tagType}}" style="{{item.styleStr}}">
      <block wx:for="{{item.nodes}}" wx:for-item="item" wx:key="">
        <template is="maxParse1" data="{{item}}" />
      </block>
    </view>

  </block>

  <!--判断是否是文本节点-->
  <block wx:elif="{{item.node == 'text'}}">
    <!--如果是，直接进行-->
    <template is="WxEmojiView" data="{{item}}" />
  </block>

</template>



